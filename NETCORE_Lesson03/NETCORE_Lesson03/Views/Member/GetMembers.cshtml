@{
}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Desc</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in ViewBag.members)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.Address</td>
            </tr>
        }
    </tbody>
   
</table>
